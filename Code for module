Sub GetTick()
    'Predefined Variables"
    Dim CellStart As Integer
    Dim CellSEnd As Integer
    Dim TickClose As Double
    Dim TickOpen As Double
    Dim TickAnswer As Double
    Dim TickChange As Double
    Dim RowCounter As Integer
    Dim VolumeAmount As Double
    Dim Greatest As Double
    Dim Least As Double
    Dim GreatestVol As Double
    Dim TickGreatest As String
    Dim TickLeast As String
    Dim TickGreatestVol As String
    
    'Set up for the tables"
    Cells(1, 9).Value = "Ticker"
    Cells(1, 10).Value = "Yearly Change"
    Cells(1, 11).Value = "Percent Change"
    Cells(1, 12).Value = "Total Stock Volume"
    Cells(1, 16).Value = "Ticker"
    Cells(1, 17).Value = "Value"
    Cells(2, 15).Value = "Greatest % Increase"
    Cells(3, 15).Value = "Greatest % Decrease"
    Cells(4, 15).Value = "Greatest Total Volume"
    
    
    'Defined Values before the Loop"
    LastRowData1 = Cells(Rows.Count, 1).End(xlUp).Row
    VolumeAmount = 0
    RowCounter = 2
    TickOpen = Cells(2, 3)
    
    'Start of the For Loop"
    For i = 2 To LastRowData1
        VolumeAmount = VolumeAmount + Cells(i, 7).Value
        If Cells(i, 1).Value <> Cells(i + 1, 1).Value Then
        'Grab the data from the range"
            TickClose = Cells(i, 6).Value
            TickAnswer = TickClose - TickOpen
            TickChange = TickAnswer / TickOpen
        'Change the Color"
        If TickAnswer < 0 Then
            Cells(RowCounter, 10).Interior.ColorIndex = 3
        Else
            Cells(RowCounter, 10).Interior.ColorIndex = 4
        End If
        'Print the answers"
            Cells(RowCounter, 9).Value = (Cells(i, 1).Value)
            Cells(RowCounter, 10).Value = TickAnswer
            Cells(RowCounter, 11).Value = FormatPercent(TickChange)
            Cells(RowCounter, 12).Value = VolumeAmount
            RowCounter = RowCounter + 1
        'Change the Values"
            VolumeAmount = 0
            TickOpen = Cells(i + 1, 3)
        End If
    Next i
    LastRowData2 = Cells(Rows.Count, 9).End(xlUp).Row
    RowCounter = 2
    Greatest = Cells(2, 11).Value
    Greatest = Cells(2, 11).Value
    GreatestVol = Cells(2, 12).Value
    'Secound Loop through new values"
    For j = 2 To LastRowData2
  
        If Cells(j + 1, 11).Value > Greatest Then
            Greatest = Cells(j, 11).Value
            TickGreatest = Cells(j, 9).Value
        End If
        If Cells(j + 1, 11).Value < Least Then
            Least = Cells(j, 11).Value
            TickLeast = Cells(j, 9).Value
        End If
        If Cells(j + 1, 12).Value > GreatestVol Then
            GreatestVol = Cells(j, 12).Value
            TickGreatestVol = Cells(j, 9).Value
        End If
    Next j
    'Printing next table"
    Cells(RowCounter, 17).Value = FormatPercent(Greatest)
    Cells(RowCounter + 1, 17).Value = FormatPercent(Least)
    Cells(RowCounter + 2, 17).Value = GreatestVol
    Cells(RowCounter, 16).Value = TickGreatest
    Cells(RowCounter + 1, 16).Value = TickLeast
    Cells(RowCounter + 2, 16).Value = TickGreatestVol
      
End Sub

